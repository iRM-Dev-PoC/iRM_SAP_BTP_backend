VIEW localized_pcf_db_Orders AS SELECT
  L.createdAt,
  L.createdBy,
  L.modifiedAt,
  L.modifiedBy,
  L.ID,
  L.book_ID,
  L.country_code,
  L.amount
FROM pcf_db_Orders AS L
WITH ASSOCIATIONS (
  MANY TO ONE JOIN localized_pcf_db_Books AS book ON (book.ID = book_ID),
  MANY TO ONE JOIN localized_sap_common_Countries AS country ON (country.code = country_code)
)