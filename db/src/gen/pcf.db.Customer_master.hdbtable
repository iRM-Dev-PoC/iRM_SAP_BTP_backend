COLUMN TABLE pcf_db_Customer_master (
  createdAt TIMESTAMP,
  createdBy NVARCHAR(255),
  modifiedAt TIMESTAMP,
  modifiedBy NVARCHAR(255),
  ID INTEGER NOT NULL,
  CUSTOMER_ID NVARCHAR(36),
  CUSTOMER_NAME NVARCHAR(5000),
  CUSTOMER_ADDRESS NVARCHAR(5000),
  START_DATE SECONDDATE,
  END_DATE SECONDDATE,
  IS_ACTIVE NVARCHAR(5000),
  CREATED_ON SECONDDATE,
  CREATED_BY NVARCHAR(5000),
  CHANGED_ON SECONDDATE,
  CHANGED_BY NVARCHAR(5000),
  PRIMARY KEY(ID)
)